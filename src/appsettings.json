{
  "forcessl": false,
  "user": {
    "username": "${user.username}",
    "password": "${user.password}",
    "salt": "${user.salt}"
  },
  "webOptimizer": {
    "enableCaching": true
  },
  "BlogOptions": {
    "commentsCloseAfterDays": 30,
    "owner": "Balázs Földesi",
    "listView": "TitlesAndExcerpts",
    "postsPerPage": 3
  },
  "cdn": {
    "url": ""
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AzureStorageOptions": {
    "ConnectionString": "${AzureTableStorageOptions.ConnectionString}",
    "TableName": "${AzureTableStorageOptions.TableName}"
  }
}
